{
    "name": "spatie/laravel-server-monitor",
    "description": "Monitor servers",
    "keywords": [
        "spatie",
        "laravel-server-monitor"
    ],
    "homepage": "https://github.com/spatie/laravel-server-monitor",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/console": "~5.4|~5.5|~5.6.0",
        "illuminate/database": "~5.4|~5.5|~5.6.0",
        "illuminate/events": "~5.4|~5.5|~5.6.0",
        "illuminate/support": "~5.4|~5.5|~5.6.0",
        "spatie/laravel-blink": "~1.0",
        "spatie/regex": "~1.1",
        "symfony/process": "~3.2|~3.3|~4.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9|0.9.5|~1.0",
        "orchestra/testbench": "~3.4|~3.5|~3.6.0",
        "phpunit/phpunit": "~5.7|~6.0|^6.3|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ServerMonitor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ServerMonitor\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\ServerMonitor\\ServerMonitorServiceProvider"
            ]
        }
    }
}
