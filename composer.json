{
    "name": "spatie/laravel-server-monitor",
    "description": "Monitor servers",
    "keywords": [
        "spatie",
        "laravel-server-monitor"
    ],
    "homepage": "https://github.com/spatie/laravel-server-monitor",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "~5.8.0",
        "spatie/laravel-blink": "~1.2.0",
        "spatie/regex": "~1.1",
        "symfony/process": "~4.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ServerMonitor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ServerMonitor\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\ServerMonitor\\ServerMonitorServiceProvider"
            ]
        }
    }
}
